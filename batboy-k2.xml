<?xml version="1.0"?>
<!DOCTYPE qualm-data SYSTEM "qualm.dtd">
<qualm-data>
  <title>Bat Boy Keyboard II</title>
  <midi-channels>
    <channel num="1">Main Keyboard</channel>
    <channel num="2">Auxiliary Keyboard</channel>
    <channel num="10">Drum Kit</channel>
  </midi-channels>
  <patches>
    <patch num="1">Nice Piano</patch>
    <patch num="2">TremStr/Str</patch>
    <patch num="3">FuzzClav/Str</patch>
    <patch num="4">Rhodes</patch>
    <patch num="5">Organ</patch>
  </patches>
  <default-trigger><trigger><foot channel="1"/></trigger></default-trigger>
  <reverse-trigger>
    <trigger><note-on channel="1" note="c-1"/></trigger>
  </reverse-trigger>
  <setup-events>
    <event><program-change channel="1" patch="1"/></event>
    <event><program-change channel="2" patch="5"/></event>
  </setup-events>

  <cue song="1a" measure="10">
    <trigger><note-on channel="1" note="C4"/></trigger>
    <event><program-change channel="1" patch="3"/></event>
  </cue>
  <cue song="2" measure="17">
    <event><program-change channel="1" patch="4"/></event>
  </cue>
</qualm-data>
